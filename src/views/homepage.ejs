<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.4/dist/jquery.slim.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
    <title>Read</title>
</head>

<body>
    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" href="/home">Home</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link" href="/create">Create</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/edit">Edit</a>
                </li>
            </ul>
            <form class="form-inline my-2 my-lg-0">
                <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
                <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
            </form>
        </div>
    </nav>
    <!-- Table -->
    <h1>
    </h1>
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">1.Id</th>
                <th scope="col">2.Email</th>
                <th scope="col">3.Password</th>
                <th scope="col">4.First Name</th>
                <th scope="col">5.Last Name</th>
                <th scope="col">6.Address</th>
                <th scope="col">7.PhoneNumber</th>
                <th scope="col">8.Gender</th>
                <th scope="col">9.Position</th>
                <th scope="col">10.Role</th>
                <th scope="col">11.ImageURL</th>
                <th scope="col">12.Create at</th>
                <th scope="col">13.Update at</th>
            </tr>
        </thead>
        <tbody>
            <% datas.forEach((data,index) => {%>
                <tr>
                    <td>
                        <%= data.id %>
                    </td>
                    <td>
                        <%= data.email %>
                    </td>
                    <td>
                        <%= data.password %>
                    </td>
                    <td>
                        <%= data.firstName %>
                    </td>
                    <td>
                        <%= data.lastName %>
                    </td>
                    <td>
                        <%= data.address %>
                    </td>
                    <td>
                        <%= data.phoneNumber %>
                    </td>
                    <td>
                        <%= data.gender %>
                    </td>
                    <td>
                        <%= data.positionId %>
                    </td>
                    <td>
                        <%= data.roleId %>
                    </td>
                    <td>
                        <%= data.image %>
                    </td>
                    <td>
                        <%= data.createdAt %>
                    </td>
                    <td>
                        <%= data.updatedAt %>
                    </td>
                </tr>
                <% });%>

        </tbody>
    </table>


</body>

</html>